model Post {
  id Int @id @default(autoincrement())
  title String
  post String 
  isPublished Boolean @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  user_id Int
  user User @relation(fields: [user_id], references: [id])
}

model User {
    id     Int @id
    posts  Post[]
}